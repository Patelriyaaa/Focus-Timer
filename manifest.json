{
    "manifest_version": 3,
    "name": "Focus Timer",
    "description": "A simple Pomodoro timer to boost productivity.",
    "version": "1.0",
    "permissions": ["storage", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  